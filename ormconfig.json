{
  "type": "sqlite",
  "database": "database.sqlite",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/entities/*.ts"
  ],
  "migrations": [
    "migrations/*.ts"
  ],
  "namingStrategy": "src/config/SnakeNamingStrategy.ts",
  "cli": {
    "entitiesDir": "src/entities",
    "migrationsDir": "migrations"
  }
}

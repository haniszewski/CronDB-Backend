{
    "type": "sqlite",
    "database": "database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
      "dist/entities/*.js"
    ],
    "migrations": [
      "dist/migrations/*.js"
    ],
    "namingStrategy": "dist/config/SnakeNamingStrategy.js",
    "cli": {
      "entitiesDir": "dist/entities",
      "migrationsDir": "dist/migrations"
    }
  }
  